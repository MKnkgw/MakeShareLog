<%=
  @title = "My Page"
  @is_home = true
  erb :"common/header"
%>

<div id="common-main">

  <div id="common-left">
    <div class="photo-list">
      <% @photo_list.each do|face| %>
        <div class="photo-block">
          <%=
            @face = face
            erb :"common/photo_block"
          %>
        </div>
      <% end %>
    </div>
  </div>
  
  <div id="common-right">
    <div class="cosmetic-list">
      <% @cosme_list.each do|cosme| %>
        <div class="cosmetic-block">
          <%=
            @cosme = cosme
            erb :"common/cosmetic_block"
          %>
      </div>
      <% end %>
    </div>
  </div>

</div>

<%= erb :"common/footer" %>
